@model AETTI.Models.Proyecto

@{
    ViewBag.Title = "Details";
}
<br>
<h2>Resultao de la busqueda</h2>

<div>
    <dl class="dl-horizontal">
        <div class="form-group col-md-12">
            @Html.LabelFor(model => model.TituloProyecto)
            @Html.TextBoxFor(model => model.TituloProyecto, new { @class = "form-control", style = "max-width:100%", @readonly = "readonly" })
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(model => model.IdTipoProyecto)
            @Html.TextBoxFor(model => model.TipoProyecto1.Descripcion, new { @class = "form-control", style = "max-width:100%;", @readonly = "readonly" })
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(model => model.Diagnostico)
            @Html.TextAreaFor(model => model.Diagnostico, new { cols = "120", rows = "4", @class = "form-control", style = "max-width:100%", @readonly = "readonly" })
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(model => model.Producto)
            @Html.TextAreaFor(model => model.Producto, new { cols = "120", rows = "4", @class = "form-control", style = "max-width:100%", @readonly = "readonly" })
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(model => model.Resumen)
            @Html.TextAreaFor(model => model.Resumen, new { cols = "120", rows = "4", @class = "form-control", style = "max-width:100%", @readonly = "readonly" })
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(model => model.Actividades)
            @Html.TextAreaFor(model => model.Actividades, new { cols = "120", rows = "4", @class = "form-control", style = "max-width:100%", @readonly = "readonly" })
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(model => model.LinkYoutube)
            @Html.TextBoxFor(model => model.LinkYoutube, new { @class = "form-control", style = "max-width:100%", @readonly = "readonly" })            
        </div>

    </dl>
</div>
<p>
    @Html.ActionLink("Volver", "Index", "Busqueda" ) 
</p>
